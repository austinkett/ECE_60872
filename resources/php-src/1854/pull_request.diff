+	if (!tmp_bufw) {
+		err = GetLastError();
+		SET_ERRNO_FROM_WIN32_CODE(err);
+		return NULL;
+	}
+
+	tmp_bufa = php_win32_ioutil_w_to_any(tmp_bufw);
+	if (!tmp_bufa) {
+		err = GetLastError();
+		buf = NULL;
+		free(tmp_bufw);
+		SET_ERRNO_FROM_WIN32_CODE(err);
+		return buf;
+	} else if (strlen(tmp_bufa) > len) {
+		free(tmp_bufa);
+		free(tmp_bufw);
+		SET_ERRNO_FROM_WIN32_CODE(ERROR_INSUFFICIENT_BUFFER);
+		return NULL;
