+static int php_mb_detect_encoding_recursive(mbfl_encoding_detector *identd, HashTable *target_hash, int *recursion_error) { /* {{{ */
+	zval *hash_entry;
+
+	if (++target_hash->u.v.nApplyCount > 1) {
